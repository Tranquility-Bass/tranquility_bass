<h1> {{title}} </h1>
<section>
    <h2>Artists Results</h2>

    {{#if emptyArtist}}
        <p>No Artists Found that match the Search Term</p>
    {{/if}}

    {{#each resultArtists}}
        <h3>
            <a href="/all/{{_id}}">{{name}}</a>
        </h3>
    {{/each}}
</section>

<section>
    <h2>Albums Results</h2>

    {{#if emptyAlbum}}
        <p>No Albums Found that match the Search Term</p>
    {{/if}}

    {{#each resultAlbums}}
        <h3>
            <a href="/all/{{_id}}">{{title}}</a>
        </h3>
    {{/each}}
</section>

<section>
    <h2>Songs Results</h2>

    {{#if emptySong}}
        <p>No Songs Found that match the Search Term</p>
    {{/if}}

    {{#each resultSongs}}
        <h3>
            <a href="/all/{{_id}}">{{title}}</a>
        </h3>
    {{/each}}
</section>