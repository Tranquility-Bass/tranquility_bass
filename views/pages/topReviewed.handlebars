<h1> {{title}} </h1>
<section>
    <h2>Top Three Artists</h2>

    {{#if emptyArtist}}
        <p>No Popular Artists Found</p>
    {{/if}}

    {{#each topArtists}}
        <h3>
            <a href="/all/{{_id}}">{{name}}</a>
        </h3>
    {{/each}}
</section>

<section>
    <h2>Top Three Albums</h2>

    {{#if emptyAlbum}}
        <p>No Popular Albums Found</p>
    {{/if}}

    {{#each topAlbums}}
        <h3>
            <a href="/all/{{_id}}">{{title}}</a>
        </h3>
    {{/each}}
</section>

<section>
    <h2>Top Three Songs</h2>

    {{#if emptySong}}
        <p>No Popular Songs Found</p>
    {{/if}}

    {{#each topSongs}}
        <h3>
            <a href="/all/{{_id}}">{{title}}</a>
        </h3>
    {{/each}}
</section>